pos0(cap_bar,90); pos7(.8hair,0);
z0=z5; x6=x7; 1.15(y6-y5l)=y0l-y7;
begingroup
  in_beak_inangle:=beak_inangle_factor * beak_inangle;
  save beak_inangle; beak_inangle=in_beak_inangle;
  p5=two_arm_path(5,6,f,0,7,g,E_mid_darkness);
endgroup;
enddef;

