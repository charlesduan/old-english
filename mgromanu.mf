 y2r=h+o; y4r=-o; y3=(.5-.2slant)[y2,y4];
 y1=min(vround max(.6h,x_height-.5vair),y2l-eps);
 %(x2l',y2l)=whatever[z2r,z1l]; x2l:=min(.5[x2l,x2l'],x2l+.5u);
 %(x4l',y4l)=whatever[z4r,z5l]; x4l:=min(x4l',x4l+.5u);
