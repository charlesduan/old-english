vardef C_barb =
 path p_[];
 pair c_barb_dir; c_barb_dir = (z2-z1) yscaled 3;
 p_1=z2l{right}...{-c_barb_dir}pen_lrend1(-c_barb_dir,fine,z6r-z1r);
 p_2=z1r{up}...{left}z2r;
 p_6=pen_rlend6(z6r-z1r,fine,z1'-z6l)--z1';
 p_1--var_combine(p_6,p_2)
enddef;

