 p1=serif_path(1,2,a,1/3,cap_jut,left);  % upper serif
 p2=serif_path(2,1,c,1/3,slant_adjusted_jut(cap_jut),left); % lower serif
 p3=internal_serif_path(1,2,3l,b,1/3,jut);
 p4=internal_serif_path(2,1,12l,d,1/3,jut);
else:
